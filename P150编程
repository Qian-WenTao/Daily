第五题
#include<stdio.h>
#include<math.h>
double f(double n);
double x;
int main()
{	
	double n = 2;
	double a;
	scanf_s("%lf", &x);
	do {
		a = f(n++);
	} while (fabs(f(n - 1) - f(n - 2)) >= 0.000001);
	printf("%lf", f(n - 1));
}
double f(double n) {
	double item,sn=1,i,fac=1,a=1;
	for (i = 1; i <= n; i++) {
		a *= 0.5 - i + 1;
		fac *= i;
		item = a / fac * pow(x, i);
		sn += item;
	}
	return sn;
}

第六题
#include<stdio.h>
#include<math.h>
void fun(int n);
int n;
int main()
{	
	scanf_s("%d", &n);
	fun(n);
}
void fun(int n) {
	int i;
	double sum = 0;
	for (i = 1; i <= n; i++) {
		if (i % 5 == 0 || i % 9 == 0)
			sum += 1.0/i; //i是int型，注意转为实型参与运算
	}
	printf("%lf", sum);
}

第七题
#include<stdio.h>
#include<math.h>
void fun(int n);
int main()
{	
	int n;
	scanf_s("%d", &n);
	fun(n);
}
void fun(int n) {
	int i, k,j;
	double sum = 0;
	for (i = 3; i <= n; i++) {
		k = (int)sqrt(i);
		for (j = 2; j <= k; j++) {
			if (i % j == 0)
				break;
		}
		if (j > k)
			sum += sqrt(i);
	}
	printf("%lf", sum);
}

第八题
#include<stdio.h>
#include<math.h>
int judge(int x);
int main()
{	
	int x;
	scanf_s("%d", &x);
	printf("有%d个", judge(x));
}
int judge(int x) {
	int i, a, b, c, count = 0;
	for (i = 100; i <= 999; i++) {
		c = i % 10;
		b = i / 10 % 10;
		a = i / 100;
		if (a + b + c == x) {
			printf("%d\n", i);
			count++;
		}
	}
	return count;
}

第九题
#include<stdio.h>
#include<math.h>
long func();
int main()
{	
	long n;
	n = func();
	printf("n=%1d\n", n);
}
long func() {
	long m;
	for (m = 0; getchar() != '@'; m++);
	return m;

}

第十题
#include<stdio.h>
#include<math.h>
int change(int n);
char array[1000];
int k = 0;
int main()
{	
	int n,i;
	scanf_s("%d", &n);
	change(n);
	for (i = k-1; i >= 0; i--) {
		printf("%c ", array[i]);
	}
}
int change(int n) {

	if (n / 10 == 0)
		return array[k++] = n + '0';
	else
		return (array[k++]=n%10+'0',change(n/10));
}
