第五题
#include<stdio.h>
#include<math.h>
double f(double n);
double x;
int main()
{	
	double n = 2;
	double a;
	scanf_s("%lf", &x);
	do {
		a = f(n++);
	} while (fabs(f(n - 1) - f(n - 2)) >= 0.000001);
	printf("%lf", f(n - 1));
}
double f(double n) {
	double item,sn=1,i,fac=1,a=1;
	for (i = 1; i <= n; i++) {
		a *= 0.5 - i + 1;
		fac *= i;
		item = a / fac * pow(x, i);
		sn += item;
	}
	return sn;
}

第七题
#include<stdio.h>
#include<math.h>
void fun(int n);
int main()
{	
	int n;
	scanf_s("%d", &n);
	fun(n);
}
void fun(int n) {
	int i, k,j;
	double sum = 0;
	for (i = 3; i <= n; i++) {
		k = (int)sqrt(i);
		for (j = 2; j <= k; j++) {
			if (i % j == 0)
				break;
		}
		if (j > k)
			sum += sqrt(i);
	}
	printf("%lf", sum);
}

第八题
#include<stdio.h>
#include<math.h>
int judge(int x);
int main()
{	
	int x;
	scanf_s("%d", &x);
	printf("有%d个", judge(x));
}
int judge(int x) {
	int i, a, b, c, count = 0;
	for (i = 100; i <= 999; i++) {
		c = i % 10;
		b = i / 10 % 10;
		a = i / 100;
		if (a + b + c == x) {
			printf("%d\n", i);
			count++;
		}
	}
	return count;
}
